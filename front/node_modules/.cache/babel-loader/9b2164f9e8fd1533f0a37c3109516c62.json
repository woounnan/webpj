\r\n      this.v_work.selects.push(this.sel)\r\n    })\r\n    var cps = this.$store.getters.getCompanys\r\n    for(var i=0; i< cps.length; i++){\r\n      if(cps[i].name === this.$store.getters.getUser.company){\r\n        this.v_divisions = cps[i].division\r\n        break\r\n      }\r\n    }\r\n    this.v_ps = this.$store.getters.getOthers\r\n  },\r\n  methods : {\r\n    initWorks(){\r\n      this.v_work = {\r\n        selects: [],\r\n        title: '',\r\n        contents: '',\r\n        startDate: '',\r\n        endDate: '',\r\n        notice: this.notice,\r\n      }\r\n      this.upFiles = []\r\n    },\r\n    popPs(idx){\r\n      this.v_work.selects.splice(idx, 1)\r\n    },\r\n    selPs(position){\r\n      this.v_work.selects.push(position)\r\n      this.v_searchPs.state = false\r\n      this.v_searchPs.cur = ''\r\n    },\r\n    uploadFile(){\r\n      console.log('call uploadFile')\r\n    },\r\n    sub(){\r\n      var ret = 0\r\n      console.log('call the sub function');\r\n      \r\n      const fd = new FormData()\r\n      fd.append('bin', document.getElementById('bin').files[0])\r\n\r\n      if(this.upFiles.length > 0 )\r\n        fd.append('flag_s_upload', true)\r\n      else\r\n        fd.append('flag_s_upload', false)\r\n      console.log('axios :::')\r\n      //파일 처리 부분(업로드, )\r\n      axios.post(`http://192.168.132.129:3000/apis/db/work`, fd)\r\n      .then(r => {\r\n        console.log('emit work')\r\n        this.$store.state.socks.sock.emit('sock_initWorks')\r\n        var works = {\r\n          title : this.v_work.title,\r\n          selects : this.v_work.selects,\r\n          contents : this.v_work.contents,\r\n          startDate : this.v_work.startDate,\r\n          endDate : this.v_work.endDate,\r\n          notice : this.v_work.notice,\r\n          by : this.$store.getters.getUser.id,\r\n          by_position : this.$store.getters.getUser.position,\r\n          state_c : this.v_work.notice ? '미확인' : '미제출',\r\n          state_s : this.v_work.notice ? '미확인' : '미제출',\r\n          flag_c_upload : false,\r\n          file_s_real : r.data.file_s_real,\r\n          file_s_save : r.data.file_s_save,\r\n        }        \r\n        console.log('selects ::: Work.vue :::', works.selects)\r\n        this.$store.state.bus.$emit('work', works)\r\n      })\r\n      .catch(e => console.error(e))\r\n      this.closeWindow();\r\n    },\r\n    closeWindow(){\r\n      console.log('call the closeWindow function');\r\n      this.$store.state.bus.$emit('closeWork')\r\n    },\r\n    searchPs(){\r\n      this.v_searchPs.ps = []\r\n      this.v_searchPs.state = false\r\n      //ps, division 합침\r\n      var arr_m = this.v_ps.concat(this.v_divisions)\r\n      var len = Object.keys(arr_m).length\r\n      for(var i=0; i<len; i++){\r\n        var temp = undefined\r\n        if(arr_m[i].position != undefined){\r\n          //ps면 \r\n          temp = arr_m[i].position\r\n        }else{\r\n          //division이면\r\n          temp = arr_m[i]\r\n        }\r\n        if( (temp.includes(this.v_searchPs.cur)) && (this.v_searchPs.cur.length > 0)){//search ps\r\n          //중복 검사, selects에 없어야 push\r\n          if(this.v_work.selects.indexOf(temp)== -1){\r\n          this.v_searchPs.ps.push(temp)\r\n          this.v_searchPs.state = true\r\n          }\r\n        }\r\n      }\r\n    },\r\n  },\r\n  created () {\r\n  }\r\n}\r\n</script>"],"sourceRoot":"node_modules/basic-vue-chat/src/components/basic-vue-chat/input"}]}